@model StockDB.Model.AspNetUserType

@{
    ViewBag.Title = "أنواع المستخدمين";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="">
    <div class="page-title">
        <div class="title_right">
            <h3>
                @ViewBag.Title<small> - حذف</small>
            </h3>
        </div>

    </div><div class="ln_solidTitle"></div>
    <div class="clearfix"></div>

    <div class="row">

        <div class="col-md-12">
            <div class="x_content">
                <div class="form-horizontal form-label-left">
                    <div class="item form-group">
                        @Html.LabelFor(model => model.Name, htmlAttributes: new { @class = "control-label col-md-3 col-sm-3 col-xs-12" })
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            @Html.DisplayFor(model => model.Name)
                        </div>
                    </div>
                    <div class="ln_solid"></div>
                    <div class="form-group">
                        <div class="col-md-6 col-md-offset-3">
                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()
                                <input type="submit" value="حذف" class="btn btn-default" />
                                @Html.ActionLink("إلغاء", "Index", "SecurityUserType", null, new { @class = "btn btn-default" })
                            }
                        </div>
                    </div>
                </div>



            </div>
        </div>
    </div>
</div>
